@using MultiplesTablesCoreApp.Models
@model Course

@{
    ViewData["Title"] = "CourseFilled";
}

<h1>The Course @Model.CourseTitle is Filled!</h1>

<div class="card p-4 shadow mb-3">
    <p>⚠️The course has no room for more students</p>
</div>
<p><a asp-action="AllCourse" asp-controller="Course" class="btn btn-primary">Back</a></p>
