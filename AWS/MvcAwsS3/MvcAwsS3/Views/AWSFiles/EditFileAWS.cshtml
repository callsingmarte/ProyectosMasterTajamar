h1 style="color:blue">Edit File AWS</h1>


<img src="~/images/@ViewData["FILENAME"]" style="width:150px;height:150px" />

<a asp-action="DeleteFileAWS"
   asp-controller="AWSFiles" class="btn btn-danger"
   asp-route-filename="@ViewData["FILENAME"]">
    <i class="fas fa-circle"></i>
    Delete
</a>
<hr />
<form method="post" enctype="multipart/form-data">
    <input type="file" name="file" class="form-control-file" /><br />
    <input type="hidden" name="filename" value="@ViewData["FILENAME"]" />
    <button type="submit" class="btn btn-success">
        Modificar archivo AWS
    </button>
</form>
<h1 style="color:blue">
    @ViewData["MENSAJE"]
</h1>
<a asp-action="Index"
   asp-controller="Home">
    Volver a Index
</a>
