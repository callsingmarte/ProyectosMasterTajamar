@using EscuelaNegocios_MartinSanchez.Models
@model Department

@{
    ViewData["Title"] = "DepartmentDetails";
}

<h1>Department Details</h1>

<div class="container">
    <div class="row mb-3">
        <div class="col-md-7 offset-3">
            <div class="form-group">
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" readonly/>
            </div>
            <div class="form-group">
                <label asp-for="Phone"></label>
                <input asp-for="Phone" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="OfficeLocation"></label>
                <input asp-for="OfficeLocation" class="form-control" readonly />
            </div>
        </div>
    </div>
    <a asp-action="Index" class="btn btn-outline-dark">Back</a>
</div>


